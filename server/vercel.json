{
    "version": 2,
    "installCommand":"npm install --legacy-peer-deps",
    "builds": [
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/vite"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }